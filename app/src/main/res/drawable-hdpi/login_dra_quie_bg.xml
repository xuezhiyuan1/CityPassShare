<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 描边的的颜色 宽度 -->
    <stroke
        android:width="0.8dp"
        android:color="@color/blue" />
    <!-- 填充的颜色 -->
    <solid android:color="@color/transparent" />
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners android:radius="10dip" />

</shape>