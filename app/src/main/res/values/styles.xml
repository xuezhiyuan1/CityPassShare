<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.




    -->
    <style name="anim_view">
        <item name="@android:windowEnterAnimation">@anim/pushwindow_alpha1</item>
        <item name="@android:windowExitAnimation">@anim/pushwindow_alpha2</item>
    </style>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.


        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:typeface">normal</item>
    </style>
    <style name="welcomeTheme" parent="android:Theme.Translucent">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowFullscreen">true</item>-->
        <!--<item name="android:windowBackground">@android:color/transparent</item>-->
    </style>
    <style name="bk_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimAmount">0.5</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="JiaodianDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Theme_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="VideoPhotoPublish">
        <item name="android:windowEnterAnimation">@anim/pop_video_photo_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_video_photo_out</item>
    </style>

    <style name="Theme_dialog_auths" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Theme_dialog_auths_add" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
    </style>

    <style name="Theme_dialog_name" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="main_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/window_in</item>
        <item name="android:windowExitAnimation">@anim/window_out</item>
    </style>

    <style name="pop_animstyle">
        <item name="android:windowEnterAnimation">@anim/pop_in</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>

    <style name="dialog_out_animstyle">
        <item name="android:windowEnterAnimation">@anim/dialog_in</item>
    </style>

    <style name="ratingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar_bg</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/ratingbar_bg</item>
        <item name="android:minHeight">20dip</item>
        <item name="android:maxHeight">20dip</item>
    </style>

    <style name="VideoTakeProgressBar" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/layer_video_take_pb</item>
    </style>

    <style name="progressbarStyle" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progressbar_bg</item>
        <item name="android:minHeight">15.0dp</item>
        <item name="android:maxHeight">15.0dp</item>
    </style>

    <style name="RadioButtonStyles">
        <item name="android:button">@drawable/ic_zhichi</item>
    </style>

    <style name="textview_16_red_text">
        <item name="android:textColor">@color/red_text</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="textview_16_back_01">
        <item name="android:textColor">@color/back_01</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="textview_16_back_02">
        <item name="android:textColor">@color/back_02</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="textview_14_back_02">
        <item name="android:textColor">@color/back_02</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="textview_12_back_02">
        <item name="android:textColor">@color/back_02</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="textview_poplist_item">
        <item name="android:textColor">@color/text_title_small</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="edittext_16_back_03">
        <item name="android:textColorHint">@color/back_03</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="BottonBarItem">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:drawablePadding">8dip</item>
        <item name="android:gravity">center|bottom</item>
        <item name="android:textSize">16sp</item>
        <item name="android:padding">5dip</item>
        <item name="android:textColor">@drawable/job_detail_top_btn</item>
        <item name="android:button">@null</item>
    </style>

    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>

    <style name="Theme.CustomDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/filled_box</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Theme.taskDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/task_dialog_shape</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="reply_namestyle">
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="sousuo_red">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#ff0000</item>
    </style>
    <style name="sousuo_999">
        <item name="android:textSize">16dp</item>
