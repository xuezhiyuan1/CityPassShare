        <item name="android:textColor">@color/color_ccc</item>
    </style>

    <style name="reply_louzhustyle">
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/bbsinformationlou</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
    </style>

    <style name="mall_style1">
        <item name="android:textColor">@color/color_666</item>
    </style>

    <style name="zan_name_span">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/color_666</item>
    </style>

    <style name="mall_style2">
        <item name="android:textColor">@color/color_333</item>
    </style>

    <style name="news_zan_style1">
        <item name="android:textColor">@color/color_666</item>
    </style>

    <style name="news_zan_style2">
        <item name="android:textColor">@color/color_999</item>
    </style>

    <style name="mes_info_style1">
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="mes_info_style2">
        <item name="android:textColor">@color/color_999</item>
    </style>

    <style name="leftImage">
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">18dp</item>
    </style>

    <!-- 进度对话框样式 -->
    <style name="CustomProgressDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="MyDialogTopRight">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <!--<item name="android:windowAnimationStyle">@style/Anim_scale</item>-->
    </style>

    <style name="Anim_scale" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/share_act_scale_in</item>
        <item name="android:activityOpenExitAnimation">@anim/share_act_scale_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/share_act_scale_in</item>
        <item name="android:activityCloseExitAnimation">@anim/share_act_scale_out</item>
    </style>

</resources>