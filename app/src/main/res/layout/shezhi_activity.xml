<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
     <include
        layout="@layout/shezhi_title"></include>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="#e6e3e3"></LinearLayout>
        <TextView
            android:id="@+id/My_Persson"
            android:layout_marginTop="18dp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:gravity="center_vertical"
            android:text="个人信息"
            />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="#e6e3e3"
        ></LinearLayout>

        <TextView
            android:layout_marginTop="18dp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:text="清空缓存"

            />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#e6e3e3"
        ></LinearLayout>
        <TextView
            android:id="@+id/Message_TiXing"
            android:layout_marginTop="18dp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:text="消息提醒"

            />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e6e3e3"
        ></LinearLayout>
        <TextView
            android:layout_marginTop="18dp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:text="黑名单管理"
            />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e6e3e3"
        ></LinearLayout>
        <TextView
            android:id="@+id/My_Passworld"
            android:layout_marginTop="18dp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:text="密码修改"

            />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:background="#e6e3e3"
        ></LinearLayout>
        <TextView
            android:layout_marginTop="18dp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="20sp"
            android:text="版本更新"
            />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e6e3e3"
        ></LinearLayout>
    <TextView
        android:id="@+id/JianYi_FanKui"
        android:layout_marginTop="18dp"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:textSize="20sp"
        android:text="建议反馈"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e6e3e3"
        ></LinearLayout>
    <TextView
        android:id="@+id/GuanYu_WoMen"
        android:layout_marginTop="18dp"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:textSize="20sp"
        android:text="关于我们"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e6e3e3"
        ></LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#e6e3e3">
        <Button
            android:id="@+id/btn_add"
            android:layout_width="fill_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/btn_userlogin"
            android:gravity="center"
            android:text="@string/btn_back_out"
            android:textColor="@color/white"
            android:textSize="20dp" />

    </LinearLayout>

</LinearLayout>